@{
    ViewData["Title"] = "ViewDB Page";
}
<div>
    <label for="fname">First name2:</label>
    <input type="text" id="fname" name="fname" onchange="myFunction()"><br><br>
</div>
<div class="text-center">
    <pre>@ViewData["db"]</pre>
</div>
<script>
    function myFunction() {
        var fname = $("#fname").val();
        alert(fname);
        $.ajax({
            method: "POST",
            url: baseUrl+"/GetKeys",
            //traditional: true, //I tried with and without that parameter
            data:
            {
                para: fname
            },
            success: function (res) {
                alert('success')
            }
        });
    }
</script>